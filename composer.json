{
  "name": "bb/magento-remote-filesystem",
  "description": "Magento 2 remote filesystem extension metapackage - contains all required dependencies to allow Magento 2 application connect to various remote filesystems like Amazon S3, Azure Blob Storage.",
  "version": "1.0.0",
  "type": "metapackage",
  "license": [
    "ADSL"
  ],
  "homepage": "https://docs.magento.asset42.com/",
  "authors": [
    {
      "name": "George Babarus",
      "email": "george.babarus+42@gmail.com",
      "homepage": "https://george.babarus.ro/"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "keywords": [
    "magento",
    "magento 2",
    "extension",
    "module",
    "filesystem",
    "remote"
  ],
  "require": {
    "php": "~7.1.3||~7.2.0||~7.3.0||~7.4.0",
    "magento/framework": ">101.0.11",
    "bb/filesystem": "^1.0",
    "bb/module-filesystem-overwrites": "^1.0",
    "bb/module-filesystem-catalog": "^1.0",
    "bb/module-filesystem-cms": "^1.0",
    "bb/module-filesystem-downloadable": "^1.0",
    "bb/module-filesystem-theme": "^1.0",
    "bb/module-filesystem-image": "^1.0"
  },
  "suggest": {
    "bb/filesystem-s3": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "composer",
      "url": "https://repo.asset42.com/"
    }
  ],
  "prefer-stable": true
}
